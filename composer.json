{
    "name": "novay/nue",
    "description": "Complete Boilerplate for Laravel.",
    "keywords": ["laravel", "ui", "boilerplate"],
    "license": "MIT",
    "homepage": "https://github.com/novay/nue",
    "authors": [
        {
            "name": "Noviyanto Rahmadi",
            "email": "novay@btekno.id"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "laravelcollective/html": "^6.3", 
        "yajra/laravel-datatables-oracle": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Novay\\Nue\\": "src/"
        },
        "files": [
            "src/Helpers.php"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Novay\\Nue\\NueServiceProvider"
            ], 
            "aliases": {
                "Nue" : "Novay\\Nue\\Facades\\Nue"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
